{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/YakShop.Mvc",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:8086",
      "sslPort": 0
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "YakShop.Mvc": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:8086"
    },
    "Docker": {
      "commandName": "IIS",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Container"
      }
    }
  }
}